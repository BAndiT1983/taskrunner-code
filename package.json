{
    "name": "taskrunnercode",
    "displayName": "TaskRunnerCode",
    "description": "Task Runner Explorer for VS Code",
    "version": "0.0.1",
    "publisher": "sana-ajani",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "task-explorer",
                    "title": "Task Explorer",
                    "icon": "assets/code.svg"
                }
            ]
        },
        "views": {
            "task-explorer": [
                {
                    "id": "task-outline",
                    "name": "Task Explorer"
                }
            ]
        }, 
        "commands": [
            {
				"command": "task-outline.addEntry",
				"title": "Add"
			}
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/jquery": "^3.3.6",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "typescript": "^3.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "jquery": "^3.3.1",
        "npm": "^6.4.0"
    }
}